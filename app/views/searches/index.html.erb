<div class="row">
  <center>
    <%= simple_form_for @search do |f| %>
      <%= f.text_field :keywords %>
    <% end %>
  </center>
</div>
<div class="row">
  <div class="clearfix mosaicflow">
    <% @items.each do |item| %>
      <div class="mosaicflow__item">
        <%= image_tag item %>
      </div>
    <% end %>
  </div>
</div>
