<div class="row">
  <%= simple_form_for @search do |f| %>
    <%= f.text_field :keywords %>
  <% end %>
</div>
<div class="row">
  <div class="clearfix mosaicflow">
    <% @items.each do |item| %>
      <div class="mosaicflow__item">
        <%= image_tag item %>
      </div>
    <% end %>
  </div>
</div>
